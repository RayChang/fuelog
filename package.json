{
  "name": "fuelog",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:pull": "turbo run db:pull",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "db:postinstall": "turbo run db:postinstall",
    "python:install": "turbo run python:install",
    "python:lint": "turbo run python:lint",
    "python:type-check": "turbo run python:type-check",
    "python:test": "turbo run python:test",
    "python:run": "turbo run python:run"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
